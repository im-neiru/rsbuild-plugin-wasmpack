{
	"name": "rsbuild-plugin-wasmpack",
	"version": "0.1.1",
	"description": "A plugin for rsbuild to compile Rust crates to WebAssembly using wasm-pack",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/im-neiru/rsbuild-plugin-wasmpack.git"
	},
	"type": "module",
	"imports": {
		"#/*": "./src/*"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "rslib build",
		"test": "playwright test",
		"format": "pnpm biome format --write",
		"lint": "biome lint --error-on-warnings",
		"fix": "biome lint --write",
		"fix-unsafe": "biome lint --write --unsafe"
	},
	"keywords": [
		"wasm",
		"rsbuild",
		"wasm-pack",
		"rspack",
		"rust"
	],
	"author": "im-neiru",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"@playwright/test": "1.57.0",
		"@rsbuild/core": "1.6.13",
		"@rslib/core": "0.18.3",
		"@types/cross-spawn": "^6.0.6",
		"@types/node": "^24.10.1",
		"playwright": "1.57.0"
	},
	"dependencies": {
		"chokidar": "^5.0.0",
		"comment-json": "^4.4.1",
		"execa": "^9.6.1",
		"js-toml": "1.0.2",
		"typescript": "5.9.3",
		"wabt": "^1.0.39"
	},
	"peerDependencies": {
		"@rsbuild/core": "^1.4.13"
	},
	"peerDependenciesMeta": {
		"@rsbuild/core": {
			"optional": true
		}
	},
	"packageManager": "pnpm@9.12.3",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	}
}
